<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@../../style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendanceLotus.controller.admin.coursesController">
    <children>
      <VBox alignment="CENTER_LEFT" prefHeight="48.0">
         <children>
            <Label text="Courses" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TableView fx:id="tableView_courses" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tableColumn_course" minWidth="-1.0" prefWidth="229.0" text="Course" />
        </columns>
      </TableView>
      <HBox>
         <children>
            <HBox alignment="CENTER_LEFT" fillHeight="false" layoutX="10.0" layoutY="10.0" prefWidth="468.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField promptText="Enter course name" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="6.0" left="6.0" right="3.0" top="6.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="button_add1" mnemonicParsing="false" onAction="#handleButtonAction" text="Add">
                     <HBox.margin>
                        <Insets bottom="6.0" left="3.0" right="6.0" top="6.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
    </children>
</VBox>
